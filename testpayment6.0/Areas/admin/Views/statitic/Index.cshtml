@model List<testpayment6._0.Areas.admin.Models.revenue_table>
@{
    ViewData["Title"] = "Thống kê doanh thu";
    Layout = "~/Areas/admin/Views/Shared/_Layout.cshtml";
}

<table class="table table-striped table-hover">

    <thead class="table-dark">
        <tr>

            <th>Mã bàn </th>
            <th>Sức chứa ban đầu</th>
            <th>Mô tả về bàn </th>
            <th>Mã khu vục</th>
            <th>Tên khu vực</th>
            <th>tổng doanh thu của bàn này </th>
            <th>Tổng số đơn đặt của bàn</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.tableId</td>
                <td>@item.capacity</td>
                <td>@item.description</td>
                <td>@item.regionId</td>
                <td>@item.regionName</td>
                <td>@item.totalRevenue</td>
                <td>@item.orderCount</td>
            </tr>
        }

    </tbody>
</table>
